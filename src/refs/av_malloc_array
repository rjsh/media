<def f='ffmpeg/libavutil/mem.h' l='206' type='void * av_malloc_array(size_t nmemb, size_t size)'/>
<doc f='ffmpeg/libavutil/mem.h' l='195'>/**
 * Allocate a memory block for an array with av_malloc().
