<fun r='_ZN6TagLib4RIFF4Info13StringHandlerD1Ev'/>
<fun r='_ZNK6TagLib4RIFF4Info13StringHandler5parseERKNS_10ByteVectorE'/>
<fun r='_ZNK6TagLib4RIFF4Info13StringHandler6renderERKNS_6StringE'/>
