<doc f='vlc/include/vlc_subpicture.h' l='128'>/**
 * Video subtitle
 *
 * Any subtitle destined to be displayed by a video output thread should
 * be stored in this structure from it&apos;s creation to it&apos;s effective display.
 * Subtitle type and flags should only be modified by the output thread. Note
 * that an empty subtitle MUST have its flags set to 0.
 */</doc>
<mbr r='subpicture_t::i_channel' o='0' t='int'/>
<mbr r='subpicture_t::i_order' o='64' t='int64_t'/>
<mbr r='subpicture_t::p_next' o='128' t='subpicture_t *'/>
<mbr r='subpicture_t::p_region' o='192' t='subpicture_region_t *'/>
<mbr r='subpicture_t::i_start' o='256' t='mtime_t'/>
<mbr r='subpicture_t::i_stop' o='320' t='mtime_t'/>
<mbr r='subpicture_t::b_ephemer' o='384' t='_Bool'/>
<mbr r='subpicture_t::b_fade' o='392' t='_Bool'/>
<mbr r='subpicture_t::b_subtitle' o='400' t='_Bool'/>
<mbr r='subpicture_t::b_absolute' o='408' t='_Bool'/>
<mbr r='subpicture_t::i_original_picture_width' o='416' t='int'/>
<mbr r='subpicture_t::i_original_picture_height' o='448' t='int'/>
<mbr r='subpicture_t::i_alpha' o='480' t='int'/>
<mbr r='subpicture_t::updater' o='512' t='subpicture_updater_t'/>
