<doc f='ffmpeg/libavcodec/twinvq_data.h' l='28'>/*
 * The bark_tab_* tables are constructed so that
 *
 *       /i-1              \
 *       |--               |
 *  bark |\   bark_tab[j]  | == i
 *       |/                |
 *       |--               |
 *       \j=0              /
 *
 *
 * for some slightly nonconventional bark-scale function
 */</doc>
